<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - UnidadeMVCCurso</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/UnidadeMVCCurso.styles.css" asp-append-version="true" />
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.0/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-KyZXEAg3QhqLMpG8r+8fhAXLRk2vvoC2f3B09zVXn8CA5QIVfZOJ3BCsw2P0p/We" crossorigin="anonymous">
    <link rel='stylesheet' href='https://cdn.jsdelivr.net/npm/vanillajs-datepicker@1.1.4/dist/css/datepicker.min.css'>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.0/dist/js/bootstrap.bundle.min.js" integrity="sha384-U1DAWAznBHeqEIlVSCgzq+c9gqGAJn5c/t99JyeKa9xxaYpSvHU5awsuZVVFIhvj" crossorigin="anonymous"></script>
    <script src='https://cdn.jsdelivr.net/npm/vanillajs-datepicker@1.1.4/dist/js/datepicker-full.min.js'></script>
</head>
<body class="main-body-maria">
    <header class="main-header-maria" style="background-color: crimson; color: crimson;">
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-crimson border-bottom box-shadow mb-3">
                
            <div class="container-fluid">
                <a class="navbar-brand" style="color:blanchedalmond;" asp-area="" asp-controller="Home" asp-action="Index">
                    <img class="Icon"src="~/LogoTech.png"loading="lazy"/>
                </a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                        
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            @*<a class="nav-link" style="color:antiquewhite;" asp-area="" asp-controller="Home" asp-action="Index">Central</a>*@
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" style="color:antiquewhite;" asp-area="" asp-controller="Users" asp-action="Index">Usuários</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" style="color:antiquewhite;" asp-area="" asp-controller="Banks" asp-action="Index">Bancos</a>
                        </li>@*
                        <li class="nav-item">
                            <a class="nav-link" style="color:antiquewhite;" asp-area="" asp-controller="Consortium" asp-action="Index">Consórcios</a>
                        </li>*@
                        <li class="nav-item">
                            <a class="nav-link" style="color:antiquewhite;" asp-area="" asp-controller="Home" asp-action="Privacy">Desenvolvedores</a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="main-footer-maria">
        <div class="container-footer" style="color:antiquewhite">
            &copy; 2022 - Projeto Integrador - Jovem Programador - <a asp-area="" asp-controller="Home" asp-action="Privacy">Desenvolvedores</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>

